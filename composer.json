{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "arrilot/laravel-widgets": "^3.12",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "fideloper/proxy": "^4.0",
        "freshbitsweb/laratables": "^1.1",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.5",
        "igaster/laravel-theme": "^2.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.7",
        "maatwebsite/excel": "^3.1",
        "nwidart/laravel-modules": "^4.0",
        "php-http/curl-client": "^1.7",
        "realrashid/sweet-alert": "^1.1",
        "sentry/sentry-laravel": "1.0.0-beta2",
        "spatie/laravel-activitylog": "^3.1",
        "spatie/laravel-model-status": "^1.6",
        "spatie/laravel-permission": "^2.29",
        "uzzaircode/password-strength": "^1.0",
        "uzzaircode/star-rating-ui":"*"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "xethron/migrations-generator": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "Datakraf\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files":[
            "app/Helpers/LayoutHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "star-rating-ui": {
            "type": "path",
            "url": "packages/uzzaircode/star-rating-ui",
            "options": {
                "symlink": true
            }
        }
    }  
}
